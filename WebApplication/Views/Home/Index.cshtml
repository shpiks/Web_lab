



<main class="container">
    <h2>@ViewData["Text"]</h2>
    <h2>Список</h2>
    <ol type="A">
    @for (int i = 1; i < 5; i++)
    {
        <li>элемент @i списка</li>
        }
    </ol>
    <hr />
    <h2>Форма</h2>
    <form method="post">
        <select asp-items="@ViewData["Lst"] as SelectList"></select>
        <br>
        <input class="forinput" type="checkbox" name="checkbox1" id="forcheckbox1">&nbsp; <label for="forcheckbox1"> Чек-бокс 1 </label><br />
        <input class="forinput" type="checkbox" name="checkbox2" id="forcheckbox2">&nbsp; <label for="forcheckbox2"> Чек-бокс 2 </label><br />
        <br>
        <input class="forinput" type="radio" name="radiobutton1" id="forradiobutton1" checked>&nbsp; <label for="forradiobutton1"> Радио-1</label>
        &nbsp;
        &nbsp;
        <input type="radio" name="radiobutton1" id="forradiobutton2">&nbsp; <label for="forradiobutton2"> Радио-2</label>
        &nbsp;
        &nbsp;
        <input type="radio" name="radiobutton1" id="forradiobutton3">&nbsp; <label for="forradiobutton3"> Радио-3</label> <br />
        <br>
        <div class="form-group row">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Логин:</label>
            <div class="col-sm-10">
                <input name="email" type="text" class="form-control" id="inputEmail3" placeholder="Введите имя">
            </div>
        </div>
        <div class="form-group row">
            <label for="inputPassword3" class="col-sm-2 col-form-label">Пароль:</label>
            <div class="col-sm-10">
                <input name="password" type="password" class="form-control" id="inputPassword3" placeholder="Введите пароль">
            </div>
        </div>
        <button name="button1" type="submit" class="btn btn-primary">Отправить</button>
    </form>
</main>

